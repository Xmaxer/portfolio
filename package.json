    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.80",
    "@mui/material": "5.6.4",
    "aws-sdk": "2.935.0",
    "ga-4-react": "0.1.281",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router": "1.0.3",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "util": "0.12.4"
  },
  "devDependencies": {
    "@craco/craco": "6.4.3",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
