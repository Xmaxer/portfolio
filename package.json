{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "build": "craco build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --fix && prettier --write . && git add .",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 && prettier --check .",
    "start": "craco start",
    "typescript": "tsc"
  },
  "pre-commit": [
    "lint",
    "typescript"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
